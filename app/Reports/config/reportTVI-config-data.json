{
        "thisFile": "reportTVI-config-data",
        "thisAbout": "Configuração de exibição dos dados do relatório de TVI's registradas no SGA",
        "reportConfigFile": "reportTVI",
        "reportBaseModel": "GerencialLancamento",
        "showToolBar": true,
        "dataDateFormat": "d/m/Y",
        "selectionData": [
                            {"columnName": "dataTransferencia", "formType": "period", "initalValue": "", "label": "Programada para"}
                         ],
      
        "columnsHide": ["nomeEmpresaOrigem", "centroCustoOrigem"],
        "columnsCustomHeader": {"nomeEmpresaOrigem": "Emp. Origem",
                                "nomeEmpresaDestino": "Emp. Destino",
                                "centroCustoOrigem": "C.Custo Origem",
                                "centroCustoDestino": "C.Custo Destino",
                                "usuarioTVI": "usuario",
                                "aprovador": "Aprovador",
                                "contaGerencial": "Conta Gerencial",
                                "valorTVI": "Valor",
                                "situacaoTVI": "Situação",
                                "operacao": "C/D",
                                "incluidaEm": "Data Inclusão",
                                "programadaPara": "Programada Para",
                                "aprovadaEm": "Aprovada Em",
                                "importada": "Importada",
                                "automatica": "Automática"},
        "columnsCustom": [],
        "columnsWrap": true,        
        "columnsFormat": [{"valorTVI":  {"horizontalAlign": "right", 
                                         "mask": "", 
                                         "dataType": "negativeNumber", 
                                         "decimalPrecision": 2}
                          }
                         ],
        "columnDb-calc": [],
        "columnGroup": {"nomeEmpresaOrigem": "Empresa de Origem: ", "centroCustoOrigem": "Centro de Custo de Origem: "},
        "columnCalculate": {"valorTVI": {"calcType": "SUM"}},
        "columnTotals": ["valorTVI"],
        "totals": {"row": "true",
                   "column": "true",
                   "group": "true",
                   "global": "true"},
        "callbackRow": "",
        "callbackColumn": {},
        "customQuery": true,
        "columnDataBaseValue":  [],
        "customData": { "table": "SGA_gerencialTvi",
                        "order": "nomeEmpresaOrigem, centroCustoOrigem",
                        "columns": [ "nomeEmpresaOrigem = empresasOrigem.nomeAlternativo, centroCustoOrigem = CCOrigem.descricaoCentroCusto, usuarioTVI = usuarioOrigem.nomeUsuario, nomeEmpresaDestino = empresasDestino.nomeAlternativo, centroCustoDestino = CCDestino.descricaoCentroCusto, aprovador = usuarioAprovador.nomeUsuario, contaGerencial = G3_gerencialContaGerencial.codigoContaGerencial+' - '+G3_gerencialContaGerencial.descricaoContaGerencial, valorTVI = SGA_gerencialTvi.valor, situacaoTVI = CASE WHEN SGA_gerencialTvi.situacao = 'A' THEN 'Aprovada' WHEN SGA_gerencialTvi.situacao = 'N' THEN 'Negada' WHEN SGA_gerencialTvi.situacao = 'S' THEN 'Aprovada Sumariamente' WHEN SGA_gerencialTvi.situacao = 'P' THEN 'Pendente de aprovação' ELSE SGA_gerencialTvi.situacao END, operacao = CASE WHEN SGA_gerencialTvi.tipoOperacao = 'D' THEN 'Débito' ELSE 'Crédito' END, incluidaEm = CONVERT(varchar, SGA_gerencialTvi.dataInclusao, 103), programadaPara = CONVERT(varchar, SGA_gerencialTvi.dataTransferencia, 103), aprovadaEm = ISNULL(CONVERT(varchar, SGA_gerencialTvi.dataAprovacao, 103), ''), importada = ISNULL(SGA_gerencialTvi.tviImportada, ''), salario = ISNULL(SGA_gerencialTvi.despesaSalario, ''), automatica = ISNULL(SGA_gerencialTvi.TVIAUTOMATICA, '')"],
                        "join":   [
                                        {   "leftTable": "G3_gerencialEmpresas",
                                            "alias" : "empresasOrigem",
                                            "leftColumn": "codigoEmpresaERP",
                                            "rightColumn": "EMP_CD_ORIGEM",
                                            "operator": "="},
                                         {  "leftTable": "G3_gerencialEmpresas",
                                            "alias" : "empresasDestino",
                                            "leftColumn": "codigoEmpresaERP",
                                            "rightColumn": "EMP_CD_DESTINO",
                                            "operator": "="},
                                        {   "leftTable": "G3_gerencialCentroCusto",
                                            "alias" : "CCOrigem",
                                            "leftColumn": "codigoCentroCustoERP",
                                            "rightColumn": "COD_CCUSTO_ORIGEM",
                                            "operator": "="},
                                        {   "leftTable": "G3_gerencialCentroCusto",
                                            "alias" : "CCDestino",
                                            "leftColumn": "codigoCentroCustoERP",
                                            "rightColumn": "COD_CCUSTO_DESTINO",
                                            "operator": "="},
                                        {   "leftTable": "SGAo_usuario",
                                            "alias" : "usuarioOrigem",
                                            "leftColumn": "COD_USUARIO",
                                            "rightColumn": "COD_USUARIO_ORIGEM",
                                            "operator": "="},
                                        {   "leftTable": "SGAo_usuario",
                                            "alias" : "usuarioAprovacao",
                                            "leftColumn": "COD_USUARIO",
                                            "rightColumn": "COD_USUARIO_APROVACAO",
                                            "operator": "="},
                                        {   "leftTable": "SGAo_usuario",
                                            "alias" : "usuarioAprovador",
                                            "leftColumn": "COD_USUARIO",
                                            "rightColumn": "usuarioAprovador",
                                            "operator": "="},
                                        {   "leftTable": "G3_gerencialContaGerencial",
                                            "leftColumn": "codigoContaGerencial",
                                            "rightColumnRAW": "REPLICATE('0', (5-LEN(SGA_gerencialTvi.COD_GRP)))+SGA_gerencialTvi.COD_GRP",
                                            "operator": "="}                                        
                                ],
                        "filter": []
                      }
        
      }